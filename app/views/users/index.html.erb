<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>


<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if current_user.email != user.email %> 
        <tr>
          <td><%= user.email %></td>
          <td><%= user.id %></td>
          <td><%= link_to 'show messages this user', user_messages_path(user.id) %></td>
          <td><%= link_to 'add to conference', new_conference_path(user.id) %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
